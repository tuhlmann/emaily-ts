{
  "name": "emaily-server",
  "version": "1.0.0",
  "description": "Emaily Server Part",
  "main": "index.js",
  "author": "Torsten Uhlmann",
  "license": "MIT",
  "engines": {
    "node": "8.11.3",
    "yarn": "1.7.0"
  },
  "scripts": {
    "tsc": "tsc",
    "clean": "rimraf build/*",
    "start:server": "cross-env NODE_ENV=production nodemon ./build/src/server",
    "start": "cd frontend && yarn build && cd .. && cross-env NODE_ENV=production nodemon ./build/src/server",
    "start:local": "yarn build && yarn start:server",
    "start:local-all": "cd frontend && yarn build && cd .. && yarn start:local",
    "build": "yarn clean && tsc",
    "dev:server": "cross-env PORT=5000 ts-node-dev ./backend/src/server",
    "dev:client": "cd frontend && yarn start",
    "dev:start": "npm-run-all -p -r dev:server dev:client",
    "heroku-postbuild": "cd frontend && cross-env NPM_CONFIG_PRODUCTION=false yarn install && yarn build && cd .. && yarn build"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "cookie-session": "^2.0.0-beta.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.4",
    "morgan": "^1.9.0",
    "nodemon": "^1.18.2",
    "npm-run-all": "^4.1.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-oauth2": "^1.4.0",
    "stripe": "^6.4.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-session": "^2.0.34",
    "@types/express": "^4.16.0",
    "@types/mongoose": "^5.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.2",
    "@types/passport": "^0.4.5",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-google-oauth2": "^0.1.2",
    "@types/passport-oauth2": "^1.4.5",
    "@types/react-hot-loader": "^4.1.0",
    "@types/stripe": "^5.0.19",
    "@types/winston": "^2.3.9",
    "prettier": "^1.13.7",
    "react-app-rewire-typescript": "^2.0.2",
    "rimraf": "^2.6.2",
    "ts-node-dev": "^1.0.0-pre.26",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0"
  }
}
